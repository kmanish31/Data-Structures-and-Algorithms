// C++ program to find n-th fibonacci
#include <iostream> 
using namespace std; 

// Approx value of golden ratio 
double golden = 1.6180339; 

int fib[6] = { 0, 1, 1, 2, 3, 5 }; 

// Function to find nth 
// Fibonacci number 
int fib (int n) 
{ 
	// Fibonacci numbers for n < 6 
	if (n < 6) 
		return f[n]; 

	// Else start counting from 
	// 5th term 
	int t = 5, fn = 5; 

	while (t < n) { 
		fn = round(fn * PHI); 
		t++; 
	} 

	return fn; 
} 


int main() 
{ 
	int n = 9; 

	cout << n << "th Fibonacci Number = "
		<< fib(n) << endl; 

	return 0; 
} 
